<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.NoteIt">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NewNotesApp">
        <activity android:name="com.example.NoteIt.Activity.UpdateNotesActivity" />
        <activity android:name="com.example.NoteIt.Activity.InsertNotesActivity" />
        <activity android:name="com.example.NoteIt.Activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.NoteIt.MainActivity" />
    </application>

</manifest>